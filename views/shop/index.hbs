{{# each products }}
    <div class="row">
        {{# each this }}
            <div class="col-sm-3 col-md-3">
                <div class="thumbnail">
                    <img src="{{this.image}}" class="img-responsive">

                    <div class="caption">
                        <h4 class="discreption">{{this.title}}.</h4>

                        <div class="clearfix">
                            <div class="price pull-left">${{this.price}}</div>
                            <a href="/buy{{this._id}}" class="btn btn-success pull-right" role="button">Buy</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
{{/each}}